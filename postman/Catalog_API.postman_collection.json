					{
						"key": "Content-Type",
						"value": "application/json",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"genderList\": [\"male\", \"female\", \"non-binary\", \"transgender\"],\n  \"pronounList\": [\"he/him\", \"she/her\", \"they/them\", \"he/they\"],\n  \"likeList\": [\n    { \"name\": \"music\" },\n    { \"name\": \"travel\" },\n    { \"name\": \"dancing\" },\n    { \"name\": \"fitness\" }\n  ],\n  \"interestList\": [\n    { \"name\": \"hiking\" },\n    { \"name\": \"photography\" },\n    { \"name\": \"dancing\" },\n    { \"name\": \"yoga\" }\n  ],\n  \"hereForList\": [\"friendship\", \"dating\", \"networking\", \"fun\"],\n  \"languageList\": [\"English\", \"Hindi\", \"Spanish\", \"French\"]\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{base_url}}/user/catalog",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"user",
						"catalog"
					]
				},
				"description": "Update entire catalog with JSON data only (no file uploads). Requires Admin or SubAdmin token.\n\nNote: To upload SVG files for likes/interests, use the \"Update Catalog (With S3 File Upload)\" endpoint instead."
			},
			"response": []
		},
