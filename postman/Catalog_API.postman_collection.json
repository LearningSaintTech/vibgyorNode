						"key": "Authorization",
						"value": "Bearer {{admin_token}}",
						"type": "text"
					},
					{
						"key": "Content-Type",
						"value": "application/json",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"listType\": \"interests\",\n  \"items\": [\"yoga\", \"art\", \"pets\"]\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
